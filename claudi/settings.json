{
  "enabledMcpjsonServers": ["claudi-memory", "claudi-rag", "claudi-image", "claudi-unified", "github"],
  "disabledMcpjsonServers": [],
  "permissions": {
    "allow": [
      "Bash",
      "Bash(*)",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "WebFetch",
      "WebSearch",
      "Task",
      "NotebookEdit",
      "mcp__claudi-memory__*",
